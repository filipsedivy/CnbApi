{
  "name": "filipsedivy/cnb-api",
  "description": "\uD83D\uDCB8 Powerful PHP library for CNB exchange rates",
  "license": "MIT",
  "authors": [
	{
	  "name": "Filip Sedivy",
	  "email": "mail@filipsedivy.cz"
	}
  ],
  "autoload": {
	"classmap": [
	  "src/"
	],
	"files": [
	  "src/exceptions.php"
	]
  },
  "require": {
	"php": ">= 7.1",
	"nette/utils": "^3.0 || ^2.5",
	"ext-mbstring": "*",
	"ext-iconv": "*"
  },
  "suggest": {
	"nette/caching": "Use if you want to retrieve data repeatedly"
  },
  "require-dev": {
	"phpstan/phpstan": "^0.11.5",
	"nette/tester": "^2.2",
	"nette/caching": "^2.5 || ^3.0"
  }
}
